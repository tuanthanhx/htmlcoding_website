{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "functions": {
    "api/send-email.js": {
      "rateLimit": {
        "enabled": true,
        "requests": 10,
        "window": "30m"
      }
    }
  }
}
